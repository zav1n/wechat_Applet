swiper swiper-item image {
  width: 100%;
  height: 100%;
}
.choice-theme text.theme-title {
  height: 50rpx;
  display: flex;
  justify-content: center;
  text-align: center;
  margin: 30rpx;
  color: #bda163;
}
.choice-theme .theme-variety .variety-tow {
  display: flex;
  justify-content: center;
  align-items: center;
}
.choice-theme .theme-variety .variety-tow navigator.variety-ruits {
  height: 370rpx;
}
.choice-theme .theme-variety .variety-tow navigator.variety-ruits image {
  height: 100%;
}
.choice-theme .theme-variety .variety-tow navigator.variety-news {
  height: 370rpx;
}
.choice-theme .theme-variety .variety-tow navigator.variety-news image {
  height: 100%;
}
.choice-theme .theme-variety navigator.variety-food image {
  width: 100%;
  height: 350rpx;
}
.recent-news text.theme-title {
  height: 50rpx;
  display: flex;
  justify-content: center;
  text-align: center;
  margin: 30rpx;
  color: #bda163;
}
.recent-news .news-list {
  display: flex;
  flex-wrap: wrap;
}
.recent-news .news-list .list-goods {
  margin: 10rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  width: 47%;
  background-color: #f1f1f1;
  border-radius: 15rpx;
  height: 310rpx;
  overflow: hidden;
  font-size: 30rpx;
}
.recent-news .news-list .list-goods image {
  width: 50%;
}
.recent-news .news-list .list-goods text {
  margin: 5rpx 0;
}
