.nav-top {
  height: 350rpx;
}
.nav-top image {
  width: 100%;
  height: 100%;
}
.nav-list .listgoods {
  display: flex;
  flex-wrap: wrap;
}
.nav-list .listgoods .list-goods {
  margin: 10rpx;
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  width: 47%;
  background-color: #f5f5f5;
  border-radius: 10rpx;
  height: 310rpx;
  overflow: hidden;
  font-size: 30rpx;
}
.nav-list .listgoods .list-goods image {
  width: 50%;
}
.nav-list .listgoods .list-goods text {
  margin: 5rpx 0;
}
